{% extends 'base.html.twig' %}

{% block title %}Offre{% endblock %}




{% block body %}

   

    <!-- Added padding-top to prevent overlap -->
    <div class="pt-5">
    
    <div class="container mt-5 pt-5">
    <h1>Offre</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ offre.id }}</td>
            </tr>
            <tr>
                <th>Ida</th>
                <td>{{ offre.ida }}</td>
            </tr>
            <tr>
                <th>Ide</th>
                <td>{{ offre.ide }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{{ offre.statut ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Descr</th>
                <td>{{ offre.descr }}</td>
            </tr>
            <tr>
                <th>Titre</th>
                <td>{{ offre.titre }}</td>
            </tr>
            <tr>
                <th>Comp</th>
                <td>{{ offre.comp }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_offre_index') }}">back to list</a>

    <a href="{{ path('app_offre_edit', {'id': offre.id}) }}">edit</a>

    {{ include('offre/_delete_form.html.twig') }}

{% endblock %}


